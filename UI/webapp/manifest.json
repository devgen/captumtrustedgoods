{
	"_version": "1.12.0",
	"sap.app": {
		"id": "rosetracker.RoseTracker",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"RoseTrackerDataService": {
				"uri": "/RoseTrackerDummyDataV2/odata/v2/RoseTrackerDataService/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "rosetracker.RoseTracker.view.Overview",
			"type": "XML",
			"async": true,
			"id": "Overview"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "rosetracker.RoseTracker.i18n.i18n"
				},
				"uri": "i18n/i18n.properties"
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "RoseTrackerDataService",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "rosetracker.RoseTracker.view",
				"controlAggregation": "pages",
				"controlId": "overview",
				"transition": "slide",
				"clearControlAggregation": false,
				"bypassed": {
					"target": "NotFound"
				}
			},
			"routes": [{
				"pattern": "",
				"name": "Overview",
				"target": ["Overview"]
			}, {
				"name": "formula",
				"pattern": "formula",
				"target": "formula"
			}, {
				"name": "details",
				"pattern": "details/{packageID}",
				"target": "details",
				"titleTarget": ""
			}],
			"targets": {
				"Overview": {
					"viewType": "XML",
					"viewName": "Overview"
				},
				"formula": {
					"viewType": "XML",
					"viewName": "Formula"
				},
				"details": {
					"viewType": "XML",
					"viewName": "Details",
					"viewLevel": 2
				},
				"NotFound": {
					"viewType": "XML",
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}