<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Panel expandable="false">
		<!-- table past packages -->
		<Table noDataText="{i18n>nodata}" id="pastPackages" items="{ path : '/PackageData' }">
			<headerToolbar>
				<Toolbar>
					<Title text=""/>
					<ToolbarSpacer/>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column id="PackageIdPast">
					<Text text="PackageID"/>
				</Column>
				<Column id="StatusPast" minScreenWidth="Tablet" demandPopin="true">
					<Text text="Status"/>
				</Column>
				<Column id="RoseTypePast" minScreenWidth="Desktop" demandPopin="false">
					<Text text="Rose Type"/>
				</Column>
				<Column id="CurrenOwnerPast" minScreenWidth="Desktop" demandPopin="true">
					<Text text="Owner"/>
				</Column>
				<Column id="LocationPast" minScreenWidth="Desktop" demandPopin="true">
					<Text text="Location"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem type="Navigation" press="handleListItemPress">
					<cells>
						<Text text="{PackageID}"/>
						<Text text="{IncidentStatus}"/>
						<Text text="{RoseType}"/>
						<Text text="{OwnerNam}"/>
						<Text text=" {path: 'OwnerLatitude'}, {path: 'OwnerLongitude'}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Panel>
</core:FragmentDefinition>