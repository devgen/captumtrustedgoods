{
  "welcomeFile": "/RoseTracker/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/RoseTrackerDataServiceFinal/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "RoseTrackerDataServiceFinal",
      "csrfProtection": false
    },
    {
      "source": "^/RoseTrackerDataService/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "RoseTrackerDataService",
      "csrfProtection": false
    },
    {
      "source": "^/RoseTracker/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}