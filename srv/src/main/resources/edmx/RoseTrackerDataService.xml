        <Property Name="MaxHumidity" Type="Edm.Decimal" Scale="4" Precision="8"/>
        <Property Name="CurrentLat" Type="Edm.Decimal" Scale="4" Precision="8"/>
        <Property Name="CurrentLong" Type="Edm.Decimal" Scale="4" Precision="8"/>
      </EntityType>
    </Schema>
  </edmx:DataServices>
